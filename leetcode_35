#35. Search Insert Position
class Solution(object):
    def binarysearch(arr, n, k):
		# code here
        lo=0
        hi=len(arr)-1
        while(lo<hi):
            mid=lo+(hi-lo)//2
            if k==arr[mid]:
                return mid
            elif k>arr[mid]:
                lo=mid+1
            else:
                hi=mid-1    
        if arr[lo]!=k :
            return -1
        elif arr[hi]!=k:
            return -1
        elif arr[lo]==k:
            return lo
        
        